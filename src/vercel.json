{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    { "key": "Access-Control-Allow-Credentials", "value": "true" },
    {
      "key": "Access-Control-Allow-Origin",
      "value": "https://ai-assistant-frontend-q6bi39n54.vercel.app"
    }
  ]
}
